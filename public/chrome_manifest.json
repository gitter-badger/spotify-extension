{
  "name": "Spotify Player",
  "version": "1.14",
  "description": "Spotify Player Chrome's extension",
  "permissions": [
    "contextMenus",
    "declarativeContent",
    "*://*.spotify.com/"
  ],
  "manifest_version": 2,
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/spotify-mini-player-16.png",
      "48": "images/spotify-mini-player-48.png",
      "128": "images/spotify-mini-player-128.png"
    },
    "default_title": "Spotify Player Extension",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/spotify-mini-player-16.png",
    "48": "images/spotify-mini-player-48.png",
    "128": "images/spotify-mini-player-128.png"
  }
}